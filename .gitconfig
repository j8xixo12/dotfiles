[core]
  editor = vim
  excludesfile = ~/.gitignore_global
[diff]
    tool = vimdiff
[push]
  default = simple
[pull]
  rebase = true
[branch]
  autosetuprebase = always
[color]
  ui = always
[color "diff"]
  meta = yellow bold
  commit = green bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red reverse
[color "diff-highlight"]
  oldNormal = red bold
  oldHighlight = "red bold 52"
  newNormal = "green bold"
  newHighlight = "green bold 22"
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "status"]
  added = green
  changed = red
  untracked = cyan
